{
    "#": {
        "max_children": 9,
        "icon": "fas fa-project-diagram",
        "valid_children":["group","project-settings"]
    },
    "group": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "far fa-object-group",
        "valid_children":["table","group-settings"]
    },
    "table": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fas fa-table",
        "valid_children":["field","table-settings"]
    },
    "field": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fas fa-stream",
        "valid_children":["field-settings"]
    },
    "field-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["none"]
    },
    "prj-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["none"]
    },
    "grp-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["none"]
    },
    "tbl-setting": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["none"]
    },
    "project-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["prj-settings"],
        "rename": false,
        "a_attr" : {
			"style": "background-color: #fff59d"
		}
    },
    "table-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["tbl-setting"],
        "rename": false,
        "a_attr" : {
			"style": "background-color: #fff59d"
		}
    },
    "group-settings": {
        "max_children": 20,
        "max_depth": 20,
        "icon": "fa fa-cog",
        "valid_children":["grp-settings"],
        "rename": false,
        "a_attr" : {
			"style": "background-color:#fff59d"
		}
    }
}