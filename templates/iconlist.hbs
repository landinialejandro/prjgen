<div class="row">
{{#each this}}
<div class="icon-list col-md-3 col-sm-6 col-12">
    <div class="info-box">
        <span class="info-box-icon bg-info"><i class="fa fa-{{this}}"></i></span>
        <div class="info-box-content">
          <span class="info-box-text">{{this}}</span>
        </div>
        <!-- /.info-box-content -->
      </div>
</div>
{{/each}}
</div>
